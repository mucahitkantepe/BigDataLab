# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                     controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file         controllers.Assets.at(path="/public", file)
#GET        /login/:id                controllers.Application.login(id)
#GET         /login2                 controllers.Application.login2

POST        /addUserInfo                       controllers.UserController.addUserInfo()
POST        /adminPanel/updateUserInfo         controllers.UserController.updateUserInfo()
POST        /adminPanel/deleteUserInfo         controllers.UserController.deleteUserInfo()
POST        /adminPanel/getUserByEmail         controllers.UserController.getUserByEmail()

POST        /adminPanel/addQuestion controllers.QuestionController.addQuestion()
POST        /adminPanel/updateQuestion controllers.QuestionController.updateQuestion()
POST        /adminPanel/deleteQuestion controllers.QuestionController.deleteQuestion()

POST        /adminPanel/addQuestionSet controllers.QuestionSetController.addQuestionSet()
POST        /adminPanel/updateQuestionSet controllers.QuestionSetController.updateQuestionSet()
POST        /adminPanel/deleteQuestionSet controllers.QuestionSetController.deleteQuestionSet()
GET         /adminPanel/getQuestionSetDetails  controllers.QuestionSetController.getQuestionSetDetails()
#
